                <div id="eqdO1awPRC0Qmih" class="bannerPosition_c4"></div>
        <script type="text/javascript">
            ready(function() {
                (function() {
                    window.addEventListener('header-bidding-loaded', function(){
                        var banner_data = {
                            banner_id:  Math.random().toString(36).substring(7)
                        };

                        var xhr = new XMLHttpRequest();
                        xhr.responseType = 'json';

                        xhr.onload = (function(position, banner_data){
                                return function(){
                                    if (xhr.status !== 200) return;
                                    var data = xhr.response;
                                    if (data.length > 0) {
                                        var banner_code = data[Math.floor(Math.random()*data.length)];
                                        banner_code = banner_code.replace(
                                            /%%([a-z_]+)%%/g,
                                            (function(banner_data){return function(str, value_name, offset, banner_code){
                                                return banner_data[value_name] ? banner_data[value_name] : '';
                                            };})(banner_data));
                                        window.setHtml(document.getElementById('eqdO1awPRC0Qmih'), banner_code);
                                    }
                                };
                            })('c4', banner_data);

                        xhr.open("GET", '/widgets/banner_loader/loader.php?type=c4');
                        xhr.send();

                    });
                })();
            });
        </script>    </div>

